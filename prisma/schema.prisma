// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model reviews {
  id        Int       @id @default(autoincrement())
  title     String
  author    String
  price     Int
  publisher String
  published String
  image     String
  read      DateTime  @default(now())
  memo      String
}
